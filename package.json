{
  "name": "dsa-tools",
  "productName": "Aventurische Enzyklopädie",
  "version": "0.0.1",
  "description": "Tools und App für 'Das Schwarze Auge'",
  "author": "Jakob Metzger",
  "license": "MIT",
  "private": true,
  "preferGlobal": true,
  "homepage": "https://github.com/JakobMe/dsa-tools#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JakobMe/dsa-tools.git"
  },
  "scripts": {
    "start": "./node_modules/.bin/electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=res//icon.icns --prune=true --out=dist",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=res/icon.ico --prune=true --out=dist",
    "package-all": "npm run package-win; npm run package-mac",
    "copy-app-local": "/bin/cp -rf './dist/Aventurische Enzyklopädie-darwin-x64/Aventurische Enzyklopädie.app' ~/Applications"
  },
  "build": {
    "appId": "com.jakobmetzger.aventurische-enzyklopaedie",
    "dmg": {},
    "win": {}
  },
  "main": "main.js",
  "bin": {
    "dsa": "./cli/cli.js"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "crawler": "^1.0.1",
    "didyoumean": "^1.2.1",
    "electron": "^1.7.4",
    "electron-squirrel-startup": "^1.0.0",
    "entities": "^1.1.1",
    "fuzzysearch": "^1.0.3",
    "jsonfile": "^2.4.0",
    "terminal-kit": "^0.26.1"
  },
  "devDependencies": {
    "electron-builder": "^11.7.0",
    "electron-packager": "^8.7.2",
    "electron-prebuilt": "^1.4.13",
    "gulp": "^3.9.1",
    "gulp-chmod": "^2.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssmin": "^0.1.7",
    "gulp-jshint": "^2.0.4",
    "gulp-less": "^3.3.0",
    "gulp-notify": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.8",
    "jshint": "^2.9.4",
    "jshint-stylish": "^2.2.1"
  }
}
