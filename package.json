{
  "name": "dsa-tools",
  "productName": "Aventurische Enzyklopädie",
  "version": "1.0.6",
  "description": "Tools und App für 'Das Schwarze Auge'",
  "author": "Jakob Metzger",
  "license": "MIT",
  "private": true,
  "preferGlobal": true,
  "homepage": "https://github.com/JakobMe/dsa-tools#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JakobMe/dsa-tools.git"
  },
  "scripts": {
    "start": "./node_modules/.bin/electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=res//icon.icns --prune=true --out=dist",
    "package-win": "electron-packager . --overwrite --asar --platform=win32 --arch=ia32 --icon=res/icon.ico --prune=true --out=dist",
    "package-all": "npm run package-win; npm run package-mac",
    "copy-app-local": "/bin/cp -rf './dist/Aventurische Enzyklopädie-darwin-x64/Aventurische Enzyklopädie.app' ~/Applications"
  },
  "build": {
    "appId": "com.jakobmetzger.aventurische-enzyklopaedie",
    "dmg": {},
    "win": {}
  },
  "main": "main.js",
  "bin": {
    "dsa": "./cli/cli.js"
  },
  "dependencies": {
    "colors": "1.3.1",
    "commander": "2.17.0",
    "crawler": "1.1.4",
    "didyoumean": "1.2.1",
    "entities": "1.1.1",
    "fuzzysearch": "1.0.3",
    "jsonfile": "4.0.0",
    "terminal-kit": "1.22.1"
  },
  "devDependencies": {
    "electron": "2.0.6",
    "electron-builder": "20.26.1",
    "electron-packager": "12.1.0",
    "electron-prebuilt": "1.4.13",
    "electron-squirrel-startup": "1.0.0",
    "gulp": "^4.0.0",
    "gulp-chmod": "2.0.0",
    "gulp-concat": "2.6.1",
    "gulp-cssmin": "0.2.0",
    "gulp-jshint": "2.1.0",
    "gulp-less": "4.0.1",
    "gulp-notify": "3.2.0",
    "gulp-rename": "1.4.0",
    "gulp-uglify": "3.0.1",
    "gulp-util": "3.0.8",
    "jshint": "2.9.6",
    "jshint-stylish": "2.2.1"
  }
}
